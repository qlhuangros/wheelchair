<?xml version="1.0" ?>
<robot name="wheelchair" xmlns:xacro="http://ros.org/wiki/xacro">
    <material name="veryDarkGrey">
        <color rgba="0.2 0.2 0.2 1.0"/>
    </material>
    <material name="light_grey">
        <color rgba="0.6 0.6 0.6 1.0"/>
    </material>
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    
    <link name="base_link">
        <inertial>
            <mass value="2"/>
            <origin xyz="0 0 0.0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.5"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.48 0.37 0.1"/>
            </geometry>
            <material name="blue"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.48 0.37 0.1"/>
            </geometry>
        </collision>
    </link>
    <link name="pole">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0.55"/>
            <geometry>
                <cylinder length="1.1" radius="0.015"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0.55"/>
            <geometry>
                <cylinder length="1.1" radius="0.015"/>
            </geometry>
        </collision>
    </link>
    <link name="wheel_base">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.43" radius="0.02"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.43" radius="0.02"/>
            </geometry>
        </collision>
    </link>
    <link name="left_wheel">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.07" radius="0.09"/>
            </geometry>
            <material name="light_grey"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.07" radius="0.09"/>
            </geometry>
        </collision>
    </link>
    <link name="right_wheel">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.07" radius="0.09"/>
            </geometry>
            <material name="light_grey"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.07" radius="0.09"/>
            </geometry>
        </collision>
    </link>
    <link name="front_wheel_base">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.57" radius="0.02"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.57" radius="0.02"/>
            </geometry>
        </collision>
    </link>
    <link name="front_left_holder">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.06 0.07 0.12"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.06 0.07 0.12"/>
            </geometry>
        </collision>
    </link>
    <link name="front_right_holder">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.06 0.07 0.12"/>
            </geometry>
            <material name="black"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.06 0.07 0.12"/>
            </geometry>
        </collision>
    </link>
    <link name="front_left_wheel">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.07"/>
            </geometry>
            <material name="light_grey"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.07"/>
            </geometry>
        </collision>
    </link>
    <link name="front_right_wheel">
        <inertial>
            <mass value="0.001"/>
            <origin xyz="0 0 0"/>
            <inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
        </inertial>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.07"/>
            </geometry>
            <material name="light_grey"/>
        </visual>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.07"/>
            </geometry>
        </collision>
    </link>
    
    <joint name="base_pole" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0.05"/>
        <parent link="base_link"/>
        <child link="pole"/>
    </joint>   
    <joint name="base_wheelbase" type="fixed">
        <origin rpy="0 0 0" xyz="-0.14 0 -0.07"/>
        <parent link="base_link"/>
        <child link="wheel_base"/>
    </joint>
    <joint name="wheelbase_leftwheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.25 0"/>
        <axis xyz="0 1 0" />
        <parent link="wheel_base"/>
        <child link="left_wheel"/>
    </joint>
    <joint name="wheelbase_rightwheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.25 0"/>
        <axis xyz="0 1 0" />
        <parent link="wheel_base"/>
        <child link="right_wheel"/>
    </joint>
    <joint name="base_frontwheelbase" type="fixed">
        <origin rpy="0 0 0" xyz="0.25 0 0.04"/>
        <parent link="base_link"/>
        <child link="front_wheel_base"/>
    </joint>
    <joint name="left_holder_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.25 -0.07"/>
        <axis xyz="0 0 1" />
        <parent link="front_wheel_base"/>
        <child link="front_left_holder"/>
    </joint>
    <joint name="right_holder_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.25 -0.07"/>
        <axis xyz="0 0 1" />
        <parent link="front_wheel_base"/>
        <child link="front_right_holder"/>
    </joint>
    <joint name="left_caster_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 -0.06"/>
        <axis xyz="0 1 0" />
        <parent link="front_left_holder"/>
        <child link="front_left_wheel"/>
    </joint>
    <joint name="right_caster_joint" type="continuous">
        <origin rpy="0 0 0" xyz="0 0 -0.06"/>
        <axis xyz="0 1 0" />
        <parent link="front_right_holder"/>
        <child link="front_right_wheel"/>
    </joint>
</robot>
